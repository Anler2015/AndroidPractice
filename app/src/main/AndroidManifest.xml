<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gejiahui.androidpractice">
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.FLASHLIGHT"/>
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.INTERNET" />
    <!--gps-->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"></uses-permission>
    <application
        android:name=".PracticeApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".viewpage.ViewPageActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".flexboxlayout.FlexBoxLayoutActivity"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name=".aidl.AIDLClientActivity"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name=".dragrecyclerview.DragRecyclerViewActivity"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name=".loadinganimation.LoadingAnimationActivity"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name=".launcher.FirstLauncherActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".notification.NotificationActivity"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name=".blur.BlurImgActivity"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name=".retrofitdemo.RetrofitActivity"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name=".gps.GpsActivity"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name=".jni.JniCallBackActivity"
            android:theme="@style/AppTheme"></activity>

        <service
            android:name=".aidl.UserManagerService"
            android:process=":remote"></service>


    </application>

</manifest>